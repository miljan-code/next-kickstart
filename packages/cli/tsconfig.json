{
  "display": "Node LTS",
  "_version": "18.1.0",
  "compilerOptions": {
    "lib": ["es2023"],
    "outDir": "dist",
    "module": "node16",
    "target": "es2022",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node16"
  },
  "include": ["src/**/*"]
}
